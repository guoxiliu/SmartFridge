-- This file contains the data types used in the system
package data_types
public
	with data_model;
	
	-- picture data type
	data picture
		properties
			data_size => 1500 KByte;
			data_model::data_representation => array;
	end picture;
	
	-- file data type
	data file
		properties
			data_model::data_representation => String;
	end file;
	
	-- content data type
	data content
		properties
			data_model::data_representation => array;
	end content;
	
end data_types;
